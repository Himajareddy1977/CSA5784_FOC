#include<stdio.h>
int main(){
	int a[10][10],b[10][10],n,m,i,j;
	printf("enter the number of rows and column size: ");
	scanf("%d%d",&n,&m);
	printf("\n-------------enter matrix--------------:");
	for(i=0;i<n;i++)
	for(j=0;j<m;j++)
		scanf("%d",&a[i][j]);
	for(i=0;i<n;i++)
	for(j=0;j<m;j++)
		b[i][j]=a[j][i];
	printf("\ntranspose matrix");
	for(i=0;i<m;i++){
	for(j=0;j<n;j++){
			printf("%d ",b[i][j]);
	}
	printf("\n");
	}
}
